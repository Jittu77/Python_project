   "source": [
    "Given a positive integer, find out the sum of all integers from 1 up to this\n",
    "number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sum(a):\n",
    "    sum1=0  #to find sum, initiate always with 0 \n",
    "    for i in range(1,a+1):\n",
    "        sum1+=i\n",
    "    return sum1\n"
   ]
  },
