   "outputs": [],
   "source": [
    "def sum1(lst1):\n",
    "    sum2=0   # to find sum intiating with 0\n",
    "    for i in lst1:\n",
    "        sum2+=i  #add each element to 0 (in form of variable sum2)\n",
    "    return sum2"
   ]
